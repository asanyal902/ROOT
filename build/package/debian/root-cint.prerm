#! /bin/sh
# prerm script for root-daemon
#
# see: dh_installdeb(1)

set -e
if [ "$1" != "upgrade" ]
then
	update-alternatives --remove cint /usr/bin/cint.root
	update-alternatives --remove makecint /usr/bin/makecint.root
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
